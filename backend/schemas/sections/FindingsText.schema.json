{
   "$schema": "https://json-schema.org/draft/2019-09/schema#",
   "additionalProperties": false,
   "metamodel_version": "1.7.0",
   "properties": {
      "FindingsText": {
         "additionalProperties": false,
         "properties": {
            "auditee_uei": {
               "allOf": [
                  {
                     "maxLength": 12,
                     "minLength": 12
                  },
                  {
                     "pattern": "^[A-H,J-N,P-Z,a-z,1-9][A-H,J-N,P-Z,a-z,0-9]+$"
                  },
                  {
                     "pattern": "^(?![A-H,J-N,P-Z,a-z,1-9]+[A-H,J-N,P-Z,a-z,0-9]*?[0-9]{9})[A-H,J-N,P-Z,a-z,0-9]*$"
                  }
               ],
               "type": "string"
            },
            "findings_text_entries": {
               "items": {
                  "additionalProperties": false,
                  "properties": {
                     "contains_chart_or_table": {
                        "enum": [
                           "Y",
                           "N"
                        ],
                        "type": "string"
                     },
                     "reference_number": {
                        "description": "Reference Number",
                        "pattern": "^20[2-9][0-9]-[0-9]{3}$",
                        "title": "ReferenceNumber",
                        "type": "string"
                     },
                     "text_of_finding": {
                        "type": "string"
                     }
                  },
                  "required": [
                     "reference_number",
                     "text_of_finding",
                     "contains_chart_or_table"
                  ],
                  "title": "FindingsTextEntry"
               },
               "type": "array"
            }
         },
         "required": [
            "auditee_uei"
         ],
         "title": "FindingsText",
         "type": "object",
         "version": 20230408
      }
   },
   "title": "UNNAMED",
   "type": "object",
   "version": 20230408
}
