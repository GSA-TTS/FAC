            "annual",
            "biennial",
            "other"
         ],
         "title": "AuditPeriod",
         "type": "string"
      },
      "UEI": {
         "allOf": [
            {
               "maxLength": 12,
               "minLength": 12
            },
            {
               "pattern": "^[A-HJ-NP-Z1-9][A-HJ-NP-Z0-9]+$"
