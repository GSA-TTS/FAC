{
   "$schema": "https://json-schema.org/draft/2019-09/schema#",
   "additionalProperties": false,
   "metamodel_version": "1.7.0",
   "properties": {
      "Meta": {
         "additionalProperties": false,
         "properties": {
            "section_name": {
               "enum": [
                  "NotesToSefa"
               ],
               "type": "string"
            },
            "version": {
               "const": "1.0.5",
               "type": "string"
            }
         },
         "required": [
            "section_name"
         ],
         "title": "Meta",
         "type": "object",
         "version": 20230807
      },
      "NotesToSefa": {
         "additionalProperties": false,
         "properties": {
            "accounting_policies": {
               "type": "string"
            },
            "auditee_uei": {
               "oneOf": [
                  {
                     "allOf": [
                        {
                           "maxLength": 12,
                           "minLength": 12
                        },
                        {
                           "pattern": "^[A-HJ-NP-Z1-9][A-HJ-NP-Z0-9]+$"
                        },
                        {
                           "pattern": "^(?![A-HJ-NP-Z1-9]+[A-HJ-NP-Z0-9]*?[0-9]{9})[A-HJ-NP-Z0-9]*$"
                        },
                        {
                           "pattern": "^(?![0-9]{9})"
                        }
                     ],
                     "type": "string"
                  },
                  {
                     "const": "GSA_MIGRATION",
                     "type": "string"
                  }
               ]
            },
            "is_minimis_rate_used": {
               "oneOf": [
                  {
                     "const": "GSA_MIGRATION",
                     "type": "string"
                  },
                  {
                     "enum": [
                        "Y",
                        "N",
                        "Both"
                     ],
                     "type": "string"
                  }
               ]
            },
            "notes_to_sefa_entries": {
               "items": {
                  "additionalProperties": false,
                  "properties": {
                     "contains_chart_or_table": {
                        "enum": [
                           "Y",
                           "N",
                           "GSA_MIGRATION"
                        ],
                        "type": "string"
                     },
                     "note_content": {
                        "type": "string"
                     },
                     "note_title": {
                        "type": "string"
                     },
                     "seq_number": {
                        "type": "integer"
                     }
                  },
                  "required": [
                     "note_title",
                     "note_content",
                     "contains_chart_or_table"
                  ],
                  "title": "NotesToSefaEntry"
               },
               "type": "array"
            },
            "rate_explained": {
               "type": "string"
            }
         },
         "required": [
            "auditee_uei",
            "accounting_policies",
            "is_minimis_rate_used",
            "rate_explained"
         ],
         "title": "NotesToSefa",
         "type": "object",
         "version": 20230713
      }
   },
   "title": "UNNAMED",
   "type": "object",
   "version": 20230713
}
