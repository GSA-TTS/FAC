{
  "name": "fac-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "npm": ">=8.0.0 <10.0.0",
    "node": ">=16.0.0 <19.0.0"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esbuild": "node build-assets.js",
    "dev": "node build-assets.js --watch",
    "check:eslint": "npx eslint --ignore-path ../.gitignore static/js",
    "fix:eslint": "npx eslint --fix --ignore-path ../.gitignore static/js",
    "check:stylelint": "npx stylelint --ignore-pattern uswds static/scss",
    "fix:stylelint": "npx stylelint --fix --ignore-pattern uswds static/scss",
    "check:prettier": "npx prettier --check .",
    "fix:prettier": "npx prettier --fix .",
    "check-all": "run-p check:*",
    "fix-all": "run-p fix:*",
    "test:a11y:pa11y": "npx pa11y-ci -c ../.pa11yci",
    "test:a11y:lighthouse": "npx lhci autorun --config='../lighthouserc.js'",
    "test:e2e:ci": "npx cypress run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.4",
    "@lhci/cli": "^0.12.0",
    "@uswds/uswds": "^3.5.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.17.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-otp": "^1.0.3",
    "esbuild-postcss": "^0.0.4",
    "esbuild-sass-plugin": "2.4.5",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^8.1.0",
    "pa11y-ci": "^3.0.1",
    "prettier": "^2.8.4",
    "stylelint": "^15.10.1",
    "stylelint-config-scss": "^1.0.0-security",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0"
  },
  "dependencies": {
    "esbuild": "^0.15.18",
    "npm": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0"
  }
}
