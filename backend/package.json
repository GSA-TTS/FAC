{
  "name": "fac-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=18.0.0 <21.0.0"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:esbuild": "node build-assets.js",
    "dev": "node build-assets.js --watch",
    "check:eslint": "npx eslint --ignore-pattern ../.gitignore static/js",
    "fix:eslint": "npx eslint --fix --ignore-pattern ../.gitignore static/js",
    "check:stylelint": "npx stylelint './static/scss'",
    "fix:stylelint": "npx stylelint --fix './static/scss'",
    "check-all": "run-p check:*",
    "fix-all": "run-p fix:*",
    "test:a11y:cypress": "npx cypress run --spec 'cypress/e2e/accessibility.cy.js'",
    "test:e2e:ci": "npx cypress run"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "cypress": "^13.8.1",
    "cypress-downloadfile": "^1.2.3",
    "cypress-axe": "^1.5.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-otp": "^1.0.3",
    "eslint": "8.57",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "prettier": "^3.2.5",
    "eslint-plugin-prettier": "^5.1.3",
    "stylelint": "^15.10.1",
    "stylelint-config-scss": "^1.0.0-security",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0"
  },
  "dependencies": {
    "@uswds/uswds": "3.7.0",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.20.2",
    "esbuild-sass-plugin": "3.0.0",
    "glob": "10.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0"
  }
}
